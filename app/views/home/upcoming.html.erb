<%- title "Upcoming Events - null Community" %>
<div class='row'>
  <div class='col-md-12'>
    <div class='page-header'>
      <h2>Upcoming Events</h2>
    </div>
  </div>
</div>

<div class='row'>
  <div class='col-md-12'>
    <% if @events.empty? %>
      <h3>
        <small>
          <center>No upcoming events.</center>
        </small>
      </h3>
    <% else %>
      <%= render :partial => 'events/list', :locals => { :events => @events } %>
    <% end %>
  </div>
</div>